{
    "name": "client",
    "version": "1.3.0",
    "scripts": {
        "build": "npm run build:pug && npm run ng -- build --prod --build-optimizer=true --statsJson=true && npm run build:version",
        "build:debug": "npm run build:pug && npm run ng -- build --prod --source-map --build-optimizer=true --statsJson=true && npm run build:version",
        "build:pug": "node scripts/build-pug.js",
        "build:version": "node scripts/version.js",
        "bundle-report": "webpack-bundle-analyzer dist/client/stats-es2015.json",
        "docker:build": "node scripts/docker/docker-build.js",
        "docker:run": "node scripts/docker/docker-run.js",
        "e2e": "npm run ng -- e2e",
        "generate:component": "./node_modules/.bin/ng generate @sbpro/ng:component",
        "generate:directive": "./node_modules/.bin/ng generate @sbpro/ng:directive",
        "generate:module": "./node_modules/.bin/ng generate @sbpro/ng:module",
        "generate:service": "./node_modules/.bin/ng generate @sbpro/ng:service",
        "lint:fix": "npm run ng -- lint --fix",
        "lint": "npm run ng -- lint",
        "ng": "cross-env NODE_OPTIONS=--max_old_space_size=2048 ./node_modules/.bin/ng",
        "serve": "./node_modules/.bin/static-server dist/client",
        "start": "node scripts/start.js",
        "test": "npm run ng -- test",
        "postinstall": "ngcc"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "13.0.2",
        "@angular/common": "13.0.2",
        "@angular/compiler": "13.0.2",
        "@angular/core": "13.0.2",
        "@angular/forms": "13.0.2",
        "@angular/platform-browser": "13.0.2",
        "@angular/platform-browser-dynamic": "13.0.2",
        "@angular/router": "13.0.2",
        "@asymmetrik/ngx-leaflet": "^8.1.0",
        "@fortawesome/angular-fontawesome": "0.10.1",
        "@fortawesome/fontawesome-svg-core": "1.2.36",
        "@fortawesome/free-brands-svg-icons": "5.15.4",
        "@fortawesome/free-regular-svg-icons": "5.15.4",
        "@fortawesome/free-solid-svg-icons": "5.15.4",
        "@ng-bootstrap/ng-bootstrap": "11.0.0-rc.0",
        "@prettier/plugin-pug": "1.17.3",
        "@types/leaflet": "^1.7.5",
        "angular-resize-event": "^3.0.2",
        "chart.js": "3.6.0",
        "file-saver": "^2.0.5",
        "font-awesome": "^4.7.0",
        "highcharts": "^9.3.1",
        "leaflet": "^1.7.1",
        "leaflet-choropleth": "^1.1.4",
        "leaflet-easybutton": "^2.4.0",
        "ngx-color-picker": "^11.0.0",
        "ngx-countup": "^7.3.3",
        "ngx-logger": "^5.0.3",
        "object-hash": "2.2.0",
        "rxjs": "7.4.0",
        "tslib": "2.3.1",
        "uuid": "8.3.2",
        "webpack-bundle-analyzer": "4.5.0",
        "zone.js": "0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "13.0.3",
        "@angular/cli": "13.0.3",
        "@angular/compiler-cli": "13.0.2",
        "@angular/language-service": "13.0.2",
        "@angular/localize": "^13.0.2",
        "@inip/static-server": "1.0.1",
        "@sbpro/ng": "1.6.1",
        "@types/chart.js": "2.9.34",
        "@types/file-saver": "^2.0.4",
        "@types/jasmine": "3.10.2",
        "@types/jasminewd2": "2.0.10",
        "@types/node": "16.11.10",
        "@types/object-hash": "2.2.1",
        "@types/uuid": "8.3.3",
        "bootstrap": "4.6.0",
        "chokidar": "3.5.2",
        "codelyzer": "6.0.2",
        "concurrently": "6.4.0",
        "cross-env": "7.0.3",
        "jasmine-core": "3.10.1",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.3.9",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "4.0.1",
        "karma-jasmine-html-reporter": "1.7.0",
        "prettier": "2.4.1",
        "protractor": "7.0.0",
        "pug": "3.0.2",
        "pug-lint": "2.6.0",
        "shelljs": "0.8.4",
        "ts-node": "10.4.0",
        "tslint": "6.1.3",
        "tslint-plugin-prettier": "2.3.0",
        "typescript": "4.4.4"
    },
    "optionalDependencies": {
        "fsevents": "^2.3.2"
    }
}
